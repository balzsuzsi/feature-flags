{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "new_dashboard": {
      "state": "ENABLED",
      "defaultVariant": "false",
      "variants": {
        "true": true,
        "false": false
      },
      "targeting": {
        "if": [
          {
            "in": [
              {
                "var": "userId"
              },
              [
                "customer-B1",
                "customer-B2"
              ]
            ]
          },
          "true",
          "false"
        ]
      }
    }
  }
}